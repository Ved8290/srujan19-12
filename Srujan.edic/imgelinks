window.addEventListener("load",function(){
  setTimeout(
    function open(event){
      document.querySelector(".popup").style.display="block";
    },1000
  )
})
 document.querySelector("#close").addEventListener("click",(e)=>{
  document.querySelector("#popup").style.display="none";
 })

